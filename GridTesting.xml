<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="Utilities.ExtentTestsReport"></listener>
     </listeners>
     
  
  <test thread-count="5" name="LinuxChromeTest">
  <parameter name="os" value="linux"></parameter>
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="TestCases.TC_LoginTest"/>
      <class name="TestCases.TC_AccountRegistrationTest"></class>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="LinuxfirefoxTest">
  <parameter name="os" value="linux"></parameter>
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="TestCases.TC_LoginTest"/>
      <class name="TestCases.TC_AccountRegistrationTest"></class>
    </classes>
  </test> <!-- Test -->
  
  
  
</suite> <!-- Suite -->
